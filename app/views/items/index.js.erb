$newelems = $("<%= j render(@items) %>");
$newelems.imagesLoaded(function() {
  $('#pinterest').append($newelems).masonry('appended', $newelems);

  <% if @items.next_page %>
    $('.pagination').replaceWith('<%= j will_paginate(@items) %>');
  <% else %>
    $('.pagination').remove();
  <% end %>

});

